# Database
DATABASE_URL="postgresql://username:password@localhost:5432/rise_of_founders?schema=public"

# Server
PORT=3001
NODE_ENV=development

# Security
JWT_SECRET="your-super-secret-jwt-key-here"
JWT_EXPIRE_TIME="7d"
BCRYPT_ROUNDS=12

# Solana
SOLANA_RPC_URL="https://api.devnet.solana.com"
SOLANA_NETWORK="devnet"

# Honeycomb Protocol
HONEYCOMB_API_KEY="your-honeycomb-api-key"
HONEYCOMB_PROJECT_ID="your-project-id"
HONEYCOMB_ENVIRONMENT="development"
HONEYCOMB_PROJECT_KEYPAIR="your-base58-encoded-project-keypair"
HONEYCOMB_EDGE_URL="https://edge.test.honeycombprotocol.com/"

# GitHub OAuth
GITHUB_CLIENT_ID="your-github-client-id"
GITHUB_CLIENT_SECRET="your-github-client-secret"
GITHUB_CALLBACK_URL="http://localhost:3001/auth/github/callback"

# OpenAI (for AI legal documents)
OPENAI_API_KEY="your-openai-api-key"
OPENAI_MODEL="gpt-4"

# Redis (for caching and sessions)
REDIS_URL="redis://localhost:6379"

# Email (for notifications)
EMAIL_HOST="smtp.gmail.com"
EMAIL_PORT=587
EMAIL_USER="your-email@gmail.com"
EMAIL_PASS="your-email-password"
EMAIL_FROM="Rise of Founders <noreply@riseoffounders.com>"

# File Storage
UPLOAD_DIR="uploads"
MAX_FILE_SIZE=10485760  # 10MB in bytes

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100

# CORS
FRONTEND_URL="http://localhost:3000"
ALLOWED_ORIGINS="http://localhost:3000,http://localhost:3001"

# Webhook URLs (for external integrations)
DISCORD_WEBHOOK_URL="your-discord-webhook-url"
SLACK_WEBHOOK_URL="your-slack-webhook-url"

# Third-party APIs
COINGECKO_API_KEY="your-coingecko-api-key"
CHAINLINK_API_KEY="your-chainlink-api-key"

# Feature Flags
ENABLE_PVP=true
ENABLE_TEAMS=true
ENABLE_SPONSORS=true
ENABLE_MENTORS=true
ENABLE_AI_AGREEMENTS=true